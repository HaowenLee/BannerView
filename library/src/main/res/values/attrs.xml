<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- ViewPager的指示器 -->
    <declare-styleable name="IndicatorLayout">
        <!-- 指示器的个数 -->
        <attr name="itemCount" format="integer"/>
        <attr name="unselectedDrawable" format="reference"/>
        <attr name="selectedDrawable" format="reference"/>
        <attr name="indicatorSize" format="dimension"/>
        <attr name="indicatorSpace" format="dimension"/>
    </declare-styleable>
</resources>